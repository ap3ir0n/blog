{
    "name": "ap3ir0n/blog",
    "description": "A blog using some cool stuff",
    "type": "project",
    "require": {
        "php": ">=7.2",
        "symfony/flex": "^1.0",
        "ramsey/uuid": "^3.7",
        "beberlei/assert": "^2.9",
        "symfony/skeleton": "^4.1",
        "league/tactician-bundle": "^1.1",
        "symfony/orm-pack": "^1.0",
        "broadway/broadway-bundle": "^0.4.1",
        "broadway/event-store-dbal": "^0.2.1",
        "ramsey/uuid-doctrine": "^1.4",
        "doctrine/doctrine-migrations-bundle": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "symfony/dotenv": "^4.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Luca Fazzi",
            "email": "patrick91@live.it"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
